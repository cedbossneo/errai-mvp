<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with type="eu.moncompte.client.NameTokens" field="nameTokens"></ui:with>

	<g:HTMLPanel>
		<g:InlineHyperlink targetHistoryToken="{nameTokens.getHomePage}">Home</g:InlineHyperlink> |
		<g:InlineHyperlink targetHistoryToken="{nameTokens.getLoginPage}">Login</g:InlineHyperlink> |
        <g:InlineHyperlink targetHistoryToken="{nameTokens.getRegisterPage}">Register</g:InlineHyperlink> |
	</g:HTMLPanel>
</ui:UiBinder> 